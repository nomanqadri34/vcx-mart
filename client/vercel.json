{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "build": {
    "env": {
      "NODE_VERSION": "20"
    }
  }
}
