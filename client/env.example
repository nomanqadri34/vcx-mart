# Frontend Environment Configuration
# Copy this file to .env.local and update the values

# API Configuration
VITE_API_URL=http://localhost:5000/api/v1

# Google OAuth (Required for Google Sign-in)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# App Configuration
VITE_APP_NAME=Cryptomart
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_GOOGLE_AUTH=true
VITE_ENABLE_CRYPTO_PAYMENTS=true
VITE_ENABLE_RAZORPAY=true

# Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here
VITE_SENTRY_DSN=your_sentry_dsn_here

# Payment Gateway Keys (These should be public keys only)
VITE_RAZORPAY_KEY_ID=your_razorpay_key_id_here
VITE_COINBASE_COMMERCE_API_KEY=your_coinbase_api_key_here

# WebSocket Configuration (Optional)
VITE_WS_URL=ws://localhost:5000

# CDN Configuration (Optional)
VITE_CDN_URL=https://your-cdn-domain.com

# Build Configuration
GENERATE_SOURCEMAP=false
REACT_APP_DISABLE_ESLINT_PLUGIN=false
